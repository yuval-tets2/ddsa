datasource postgres {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt DateTime           @default(now())
  firstName String?
  id        String             @id @default(cuid())
  lastName  String?
  newfield  EnumUserNewfield[]
  password  String
  roles     Json
  updatedAt DateTime           @updatedAt
  username  String             @unique
}

model Customer {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}

enum EnumUserNewfield {
  Vxcvxc
  Vcxv
}